{
  "name": "suminixmed",
  "version": "1.3.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:legacy": "next dev",
    "dev:local": "NEXTAUTH_URL=http://localhost:3000 next dev",
    "dev:network": "NEXTAUTH_URL=http://$(ipconfig getifaddr en0 || ipconfig getifaddr en1):3000 next dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "npm run lint:eslint && npm run lint:types",
    "lint:eslint": "eslint \"app/**/*.{js,jsx,ts,tsx,mjs}\" \"lib/**/*.{js,jsx,ts,tsx,mjs}\" \"hooks/**/*.{js,jsx,ts,tsx,mjs}\" --ext .js,.jsx,.ts,.tsx,.mjs --ignore-pattern '.next/**' --ignore-pattern 'node_modules/**' --ignore-pattern 'out/**' --ignore-pattern 'dist/**' --ignore-pattern 'migration-package/**' --ignore-pattern 'public/backup/**' --ignore-pattern 'backups/**' --max-warnings 0",
    "lint:types": "tsc --noEmit",
    "lint:fix": "eslint \"app/**/*.{js,jsx,ts,tsx,mjs}\" \"lib/**/*.{js,jsx,ts,tsx,mjs}\" \"hooks/**/*.{js,jsx,ts,tsx,mjs}\" --ext .js,.jsx,.ts,.tsx,.mjs --fix --ignore-pattern '.next/**' --ignore-pattern 'node_modules/**'",
    "lint:report": "eslint \"app/**/*.{js,jsx,ts,tsx,mjs}\" \"lib/**/*.{js,jsx,ts,tsx,mjs}\" \"hooks/**/*.{js,jsx,ts,tsx,mjs}\" --ext .js,.jsx,.ts,.tsx,.mjs --format json --output-file eslint-report.json",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx,json,css,md}\" \"lib/**/*.{js,jsx,ts,tsx}\" \"hooks/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"app/**/*.{js,jsx,ts,tsx,json,css,md}\" \"lib/**/*.{js,jsx,ts,tsx}\" \"hooks/**/*.{js,jsx,ts,tsx}\"",
    "verify": "node scripts/verify-code.mjs",
    "pre-commit": "npm run verify",
    "seed": "node prisma/seed.mjs",
    "test:integration:lotes": "node ./scripts/integration/test-lotes.mjs",
    "sync:modules": "node scripts/sync-rbac-modules.mjs",
    "sync:modules:cleanup": "node scripts/sync-rbac-modules.mjs --cleanup",
    "rbac:verify": "node verificar-permisos-duplicados.mjs",
    "rbac:clean": "node limpiar-permisos-obsoletos.mjs",
    "rbac:sync": "node sync-all-permissions-rol.mjs",
    "migrate:compare-schemas": "node scripts/comparar-esquemas.mjs",
    "migrate:dry-run": "node scripts/migrar-datos-produccion.mjs --dry-run",
    "migrate:execute": "node scripts/migrar-datos-produccion.mjs --execute",
    "migrate:backup": "PGPASSWORD=1803 pg_dump -h localhost -p 5432 -U postgres -d suminix -f backups/desarrollo-manual-$(date +%Y%m%d-%H%M%S).sql && echo 'âœ… Backup creado en backups/'",
    "migrate:validate": "node scripts/validar-post-migracion.mjs"
  },
