    "date-fns": "^4.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "next": "^15.5.9",
    "next-auth": "^4.24.11",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "xlsx": "^0.18.5",
